{
  "name": "react-sandbox-v2",
  "version": "0.0.1",
  "description": "Updated version of the React-Sandbox with updated packages and methods",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development && rimraf ./build/* && webpack-dev-server --mode development --open --hot",
    "build:prod": "rimraf ./build/* && webpack --mode production --progress",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "immutable": "3.8.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "5.2.0",
    "react-intl": "2.6.0",
    "react-redux": "5.0.7",
    "react-redux-toastr": "7.4.0",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-observable": "1.0.0",
    "rxjs": "6.3.2"
  },
  "devDependencies": {
    "@storybook/react": "3.4.11",
    "autoprefixer": "9.1.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-react": "6.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "3.1.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "node-sass": "4.9.3",
    "postcss-loader": "3.0.0",
    "prettier": "1.14.2",
    "react-hot-loader": "4.3.8",
    "react-loadable": "5.5.0",
    "react-router-dom": "4.3.1",
    "rimraf": "2.6.2",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.1",
    "styled-components": "3.4.2",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.5"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "react-hot-loader/babel",
      "syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "standard",
      "standard-react"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": 0
    }
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQute": true,
    "trailingComma": "none"
  }
}
